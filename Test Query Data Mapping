USE `testdb`;

SELECT 
    p.ProductID,
    p.FamilyID,
    p.HeroImage,
    p.ProductCode,
    p.ProductGroupID,
    p.SubFamilyID,
    p.SupplierID,
    f.FamilyDescription,
    f.FamilyName,
    f.FamilyShortDescription,
    sf.SubFamilyDescription,
    sf.SubFamilyName,
    sf.SubFamilyShortDescription,
    pg.Collection,
    pg.ProductCategory,
    pg.ProductSubCategory,
    pc.HousingMaterial,
    pc.OpticMaterial,
    pc.Weight,
    pc.MiscellaneousInfo AS PhysicalMiscInfo,
    pc.FinishColor,
    pc.FinishType,
    pc.MountingType,
    pc.Dimensions,
    pc.CutOutSize,
    pc.DimensionDiagram,
    pc.FinishIcon,
    es.AdvertisedWatts,
    es.ConnectionDescription,
    es.Current,
    es.Cycles,
    es.ElectricalClass,
    es.LCP,
    es.MiscellaneousInfo AS ElectricalMiscInfo,
    es.PowerFactor,
    es.RatedVoltage,
    es.TaMax,
    es.TaMin,
    lc.CCT,
    lc.CRI,
    lc.ChipSeries,
    lc.MiscellaneousInfo AS LEDMiscInfo,
    lc.PrimaryChipSupplier,
    lc.SecondaryChipSupplier,
    lc.ChipCode,
    lc.ChipVoltageRange,
    lc.ChipForwardVoltageRating,
    lc.ChipForwardCurrentRating,
    lc.ArrayForwardVoltageRating,
    lc.ArrayForwardCurrentRating,
    lc.ArrayOutput,
    lc.ArrayZhagaType,
    lc.R9Value,
    lc.DUV,
    lc.WhitePointX,
    lc.WhitePointY,
    lc.PeakWaveLength,
    lc.MOverPRatio,
    lc.SPRatio,
    lc.TM30Report,
    lc.FidelityIndex,
    lc.GamutIndex,
    lc.LocalChromaShift,
    lc.LocalHueShift,
    lc.LocalColorFidelity,
    lc.PLevel,
    lc.VLevel,
    lc.FLevel,
    lc.LocalAverages,
    lc.SDCMOverLife,
    lc.SDCMInitial,
    lc.RatedLife,
    lc.InSituTempOfLEDChip,
    lc.LEDModuleVf,
    lc.LumenPercentage50000Hrs,
    ds.VersaluxDriverCode,
    ds.Description AS DriverDescription,
    ds.DriverSupplier,
    ds.Control,
    ds.WattsLoss,
    ds.ForwardCurrentRating,
    ds.ForwardVoltageRating,
    ds.DriverSetting,
    ds.MiscellaneousInfo AS DriverMiscInfo,
    ds.InputVoltageRangeMin,
    ds.InputVoltageRangeMax,
    ds.AOCIfRangeMin,
    ds.AOCIfRangeMax,
    ds.TypeCCurrent,
    ds.TypeCVoltage,
    ds.FixedOutput,
    ds.Compatibility,
    ds.ElectricalProtectionClass,
    ds.DriverMounting,
    ds.IP,
    ds.TaC,
    ds.CLO,
    ds.VfRangeMin,
    ds.VfRangeMax,
    ds.SELVHV,
    ds.ExtNTC,
    ds.InrushCurrent,
    ds.InrushTWidth,
    ds.Surge,
    ds.PowerFactor AS DriverPowerFactor,
    ds.LeakageCurrent,
    ds.THDPercentage,
    ds.PstLM,
    ds.SVM,
    ds.MaxOperatingTemp,
    ca.Approvals,
    ca.ATEX,
    ca.B0FireResistance,
    ca.B15FireResistance,
    ca.Battery,
    ca.CeilingCoveringRating,
    ca.DriveOverRating,
    ca.EmergencyRating,
    ca.HazardousRating,
    ca.IKRating,
    ca.IPRating,
    ca.MiscellaneousInfo AS CertificateMiscInfo,
    ca.OperatingTempMax,
    ca.OperatingTempMin,
    ca.RCM,
    ca.SeismicRating,
    ca.WalkOverRating,
    ca.Warranty,
    ies.IESFile,
    ies.IESFileDescription,
    ies.IESFileName,
    os.BeamAngles,
    os.BeamSpread,
    os.BUGRating,
    os.CartesianGraph,
    os.Category,
    os.ConeLuxDiagram,
    os.Description AS OpticalDescription,
    os.DLCMaxUGR,
    os.DownwardOutputAdvertised,
    os.DownwardOutputReal,
    os.Efficacy,
    os.FieldAngles,
    os.FixtureOutputAdvertised,
    os.FixtureOutputReal,
    os.IESClassification,
    os.IESFileTableID,
    os.L70,
    os.L80,
    os.L90,
    os.LCSGraph,
    os.LongitudinalClassification,
    os.LuminousDimensions,
    os.MaxIntensity,
    os.MaxIntensityAngleH,
    os.MaxIntensityAngleV,
    os.MaxUGR,
    os.NEMAType,
    os.PolarCurve,
    os.SpacingCriteria,
    os.UpwardOutputAdvertised,
    os.UpwardOutputReal,
    os.UWLR,
    a.AccessoryCode,
    a.AccessoryConstruction,
    a.AccessoryDescription,
    a.AccessoryDimensionalDiagram,
    a.AccessoryHeroImage,
    a.AccessoryShortDescription,
    a.AccessorySymbol,
    a.MiscellaneousInfo AS AccessoryMiscInfo
FROM
    Products p
LEFT JOIN Families f ON p.FamilyID = f.FamilyID
LEFT JOIN SubFamilies sf ON p.SubFamilyID = sf.SubFamilyID
LEFT JOIN ProductGroups pg ON p.ProductGroupID = pg.ProductGroupID
LEFT JOIN PhysicalConstructions pc ON p.ProductID = pc.ProductID
LEFT JOIN ElectricalSpecifications es ON p.ProductID = es.ProductID
LEFT JOIN LEDCharacteristics lc ON p.ProductID = lc.ProductID
LEFT JOIN DriverSpecifications ds ON p.ProductID = ds.ProductID
LEFT JOIN CertificateApprovals ca ON p.ProductID = ca.ProductID
LEFT JOIN IESFiles ies ON p.ProductID = ies.ProductID
LEFT JOIN OpticalSpecifications os ON p.ProductID = os.ProductID
LEFT JOIN Accessories a ON p.ProductID = a.ProductID
ORDER BY p.ProductID;
